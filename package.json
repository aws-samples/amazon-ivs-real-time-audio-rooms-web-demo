{
  "name": "amazon-ivs-real-time-audio-only-web-demo-private",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode $APP_ENV",
    "preview": "vite preview --config vite.config.preview.ts",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.json --max-warnings=0 --report-unused-disable-directives",
    "lint:fix": "npx concurrently npm:tsc:* npm:eslint:fix",
    "lint": "npx concurrently npm:tsc:* npm:eslint",
    "prelint:fix": "npx prettier --write .",
    "prelint": "npx prettier --check .",
    "rotateKeyPair": "tsx scripts/rotateKeyPair.ts",
    "deploy:init": "npx concurrently --kill-others-on-fail \"npm ci\" \"npm ci --prefix infra\" && npm run deploy:bootstrap",
    "deploy:bootstrap": "npm run bootstrap --prefix infra",
    "predeploy:website": "npm run build",
    "deploy:website": "npm run deploy --prefix infra -- -c stackType=website",
    "deploy:website:dev": "APP_ENV=development npm run deploy:website",
    "deploy:website:prod": "APP_ENV=production npm run deploy:website",
    "deploy:backend": "npm run deploy --prefix infra -- -c stackType=backend",
    "deploy:backend:dev": "APP_ENV=development npm run deploy:backend",
    "deploy:backend:prod": "APP_ENV=production npm run deploy:backend",
    "destroy:website": "npm run destroy --prefix infra -- -c stackType=website",
    "destroy:backend": "npm run destroy --prefix infra -- -c stackType=backend",
    "clean": "rm -rf build dist infra/dist infra/cdk.out"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "@tailwindcss/container-queries": "^0.1.1",
    "amazon-ivs-web-broadcast": "^1.15.0",
    "axios": "^1.7.4",
    "boring-avatars": "1.10.2",
    "copy-to-clipboard": "^3.3.3",
    "fast-equals": "^5.0.1",
    "fast-memoize": "^2.5.2",
    "framer-motion": "^11.3.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-modal": "^3.16.1",
    "react-qrcode-logo": "^3.0.0",
    "react-resize-detector": "^11.0.1",
    "react-router-dom": "^6.26.0",
    "react-tooltip": "^5.28.0",
    "swr": "^2.2.5"
  },
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.624.0",
    "@aws-sdk/client-lambda": "^3.645.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.9",
    "tailwindcss-themer": "^4.0.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --report-unused-disable-directives --fix",
    "*.{yml,yaml,md}": "prettier --write",
    "*.css": "prettier --write --parser=css",
    "!(package|tsconfig|cdk)*.json": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ]
  }
}